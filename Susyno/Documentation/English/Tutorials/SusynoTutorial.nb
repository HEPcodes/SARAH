Cell[CellGroupData[{

Cell["Interpreting the output", "Section",
 CellID->21141493],

Cell[TextData[{
 "After executing the ",
 Cell[BoxData[
  ButtonBox["BetaFunctions1L",
   BaseStyle->"Link",
   ButtonData->"paclet:Susyno/ref/BetaFunctions1L"]], "InlineFormula"],
 " / ",
